{
  "presets": [
    ["env", {
      "modules": false,
      "useBuiltIn": true,
      "node": true,
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "not ie <= 11",
        ]
      }
    }],
    "react",
    "stage-0",
    "flow"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
      ]
    }
  },
  "plugins": [
    "react-hot-loader/babel",
    ["module-resolver", {
      "alias": {
        "@": "./app"
      }
    }],
  ],
}
